CHIP Log2
{
    IN a[16];
    OUT out[16];
    
    PARTS:
    
    Clzv2(a=a , out=v);

    Not16(in=v , out= nv1);
    Add16(a=nv1, b[0]=true, b[1..15]=false, out=nv);
    Add16(a[0..3]=true, a[4..15]=false, b=nv , out = reza);
    And(a=a[15], b=true, out=p);

    Mux16(a=reza ,b=true ,  sel=p , out=out);
    
}